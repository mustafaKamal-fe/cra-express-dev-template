{
	"name": "multer-express",
	"version": "0.1.0",
	"description": "This is how you upload files using multer and express.",
	"private": false,
	"engines": {
		"node": "12.x"
	},
	"scripts": {
		"server": "nodemon server",
		"client": "cd client && yarn start",
		"start:dev": "concurrently \"yarn server\" \"yarn client\"",
		"start": "node server",
		"build": "cd client/ && yarn && yarn build"
	},
	"cacheDirectories": [
		"node_modules",
		"client/node_modules"
	],
	"dependencies": {
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"multer": "^1.4.2",
		"nodemon": "^2.0.7"
	},
	"devDependencies": {
		"@types/node": "^16.4.1",
		"concurrently": "^6.2.0",
		"typescript": "^4.3.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mustafaKamal-fe/multer-express"
	},
	"keywords": [
		"node",
		"heroku",
		"create-react-app",
		"react",
		"express",
		"multer",
		"formData",
		"file",
		"upload"
	],
	"license": "MIT",
	"author": {
		"name": "Mustafa Kamal",
		"email": "gistcodee@gmail.com",
		"url": "https://github.com/mustafaKamal-fe/"
	}
}
